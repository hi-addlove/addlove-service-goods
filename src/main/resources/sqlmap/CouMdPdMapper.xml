        ZMWCOST, 
        ZMSCOST, 
        SJCOUNT, 
        SJHCOST, 
        SJWCOST, 
        SJSCOST, 
        YKCOUNT, 
        YKHCOST, 
        YKWCOST, 
        YKSCOST, 
        DATASTATUS, 
        REMARK
    )
    VALUES
    (
        #{billNo,jdbcType=VARCHAR},
        to_date(#{lrDate,jdbcType=DATE},'yyyy-MM-dd HH24:mi:ss'),
        #{userId,jdbcType=NUMERIC},
        #{userCode,jdbcType=VARCHAR},
        #{userName,jdbcType=VARCHAR},
        #{orgCode,jdbcType=VARCHAR},
        #{orgName,jdbcType=VARCHAR},
        #{inOrgCode,jdbcType=VARCHAR},
        #{depId,jdbcType=NUMERIC},
        #{depCode,jdbcType=VARCHAR},
        #{depName,jdbcType=VARCHAR},
        #{ckCode,jdbcType=VARCHAR},
        #{ckName,jdbcType=VARCHAR},
        #{ywType,jdbcType=VARCHAR},
        #{billType,jdbcType=VARCHAR},
        #{couStatus,jdbcType=VARCHAR},
        #{zmCount,jdbcType=NUMERIC},
        #{zmHCost,jdbcType=NUMERIC},
        #{zmWCost,jdbcType=NUMERIC},
        #{zmSCost,jdbcType=NUMERIC},
        #{sJCount,jdbcType=NUMERIC},
        #{sJhCost,jdbcType=NUMERIC},
        #{sJwCost,jdbcType=NUMERIC},
        #{sJsCost,jdbcType=NUMERIC},
