    Select OptionValue As pdType From tSysOption 
    Where SysOption = 'Cou_LpDealType' And Section = 'COU' And OrgCode = #{orgCode,jdbcType=VARCHAR}
  </select>
  
  <!-- 执行生产完工存储过程 -->
  <select id="execStartPdProcedure" statementType="CALLABLE" parameterType="java.util.HashMap">
    <![CDATA[
     {
       CALL sCou_MdPd_Start_CDADL
       (
         #{ps_BillNo,mode=IN,jdbcType=VARCHAR},
         #{pi_Result,mode=OUT,jdbcType=NUMERIC}, #{ps_Message,mode=OUT,jdbcType=VARCHAR}
       )
     }
    ]]>
  </select>
  
